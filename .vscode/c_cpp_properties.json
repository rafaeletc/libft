{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "linux-gcc-x64",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "/usr/include"
                ]
            },
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-Werror"
            ]
        }
    ],
    "version": 4
}
